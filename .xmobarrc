Config { overrideRedirect = False
       , font     = "xft:iosevka-18"
       , additionalFonts = [ "xft:iosevka-17" ]
       , bgColor  = "#282828"
       , fgColor  = "#96a6c8"
       , position = TopSize L 100 45
       , commands = [ Run XMonadLog
                    , Run Locks
                    , Run Cpu
                        [ "-L", "3"
                        , "-H", "50"
                        , "--high"  , "red"
                        , "-t", "<fc=#ffdd33></fc> <total>%"
                        ] 10
                    , Run Memory ["-t", "<fc=#ffdd33></fc> <usedratio>%"] 10
                    , Run Alsa "default" "Master"
                        [ "-t", "<fc=#ffdd33><fn=1></fn></fc> <volumestatus>"
                        , "--suffix"  , "True"
                        , "--"
                        , "--on", ""
                        ]
                    , Run Battery [ "-t", "<fc=#ffdd33></fc> <left>% <acstatus>" ] 60
                    , Run Wireless "wlp0s20f3" [ "-t", "<fc=#ffdd33><fn=1></fn></fc> <essid> (<quality>)" ] 10
                    , Run Date "<fc=#ffdd33></fc> %H:%M" "date" 10
                    ]
       , sepChar  = "%"
       , alignSep = "}{"
       , template = " <fc=#ffdd33></fc> %XMonadLog% }{ %locks%  %cpu%  <fc=#52494e>|</fc>  %memory%  <fc=#52494e>|</fc>  %alsa:default:Master%  <fc=#52494e>|</fc>  %battery%  <fc=#52494e>|</fc>  %wlp0s20f3wi%  <fc=#52494e>|</fc>  %date% "
       }
